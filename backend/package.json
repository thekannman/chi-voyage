{
  "name": "chivoyage-backend",
  "version": "1.0.0",
  "description": "Backend for Chi Voyage - Chicago's Neighborhood Explorer",
  "main": "dist/server.js",
  "scripts": {
    "start": "NODE_ENV=production PORT=3001 node dist/server.js",
    "dev": "NODE_ENV=development PORT=3001 ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "jest",
    "migrate": "ts-node src/scripts/migrate.ts",
    "migrate:create": "ts-node src/scripts/create-migration.ts",
    "import-data": "ts-node src/scripts/import-data.ts",
    "sync": "ts-node src/sync-manager.ts",
    "migrate:down": "ts-node -e \"require('./src/db/migrator').rollbackLatest()\"",
    "seed:neighborhoods": "ts-node src/seed-neighborhoods.ts",
    "associate:neighborhoods": "ts-node src/associate-neighborhoods.ts",
    "scheduler": "ts-node src/scheduler.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "cleanup-duplicates": "ts-node src/scripts/cleanup-duplicates.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.71.5",
    "@types/node": "^20.11.19",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.11",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "kysely": "^0.27.6",
    "node-cron": "^3.0.3",
    "outscraper": "^2.0.0",
    "pg": "^8.14.1",
    "react-intersection-observer": "^9.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
